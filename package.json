{
  "name": "ts-starter",
  "version": "1.0.0",
  "description": "There are many like it, but this one is mine.",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "build:prod": "rollup -c && NODE_ENV=production rollup -c",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'tsx src/index.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs:build": "typedoc --out docs ./src",
    "docs:clean": "rm -rf docs",
    "docs:serve": "npx http-server docs -o",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rollup": "^2.79.1",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "tsx": "^4.10.5",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  }
}
